{
  "name": "@myflags/core",
  "version": "0.3.0",
  "description": "Core SDK for MyFlags feature flag management",
  "author": "MyFlags Team",
  "license": "MIT",
  "homepage": "https://github.com/myflags-io/sdk#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/myflags-io/sdk.git",
    "directory": "packages/core"
  },
  "keywords": [
    "feature-flags",
    "feature-toggles",
    "flags",
    "sdk"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "publishConfig": {
    "access": "public"
  },
  
  "dependencies": {},
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "eslint": "^9.24.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.29.1",
    "vitest": "^1.2.2",
    "@vitest/coverage-v8": "^1.2.2",
    "jsdom": "^24.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}